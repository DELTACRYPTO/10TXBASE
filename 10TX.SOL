pragma solidity ^0.8.0;

contract MultiTx {
    address public owner;

    constructor() {
        owner = msg.sender;
    }

    event TransactionExecuted(string txName, address sender);

    function tx1() public {
        emit TransactionExecuted("tx1", msg.sender);
    }

